{
  "name": "nobullshitcooking-nxt",
  "version": "1.0.0",
  "description": "Next.js-based UI for No Bullshit Cooking",
  "author": "Timothy James Alferes",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --color --detectOpenHandles"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsconfig": "<rootDir>/tsconfig.jest.json"
      }
    },
    "globalSetup": "./global-setup.js",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy",
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-multi-backend/dist/esm/HTML5toTouch$": "react-dnd-multi-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    },
    "preset": "ts-jest/presets/js-with-babel",
    "setupFiles": [
      "<rootDir>/tests/setupTests.ts"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/.next",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/*.test.tsx?$",
    "transform": {
      "^.+\\.[t]sx?$": "ts-jest",
      "^.+\\.[j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@elastic/react-search-ui": "^1.8.0",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.24.0",
    "date-fns": "^2.27.0",
    "fibers": "^5.0.0",
    "focus-trap-react": "^8.9.0",
    "image-extensions": "^1.1.0",
    "immutability-helper": "^3.1.1",
    "is-url": "^1.2.4",
    "next": "12.0.7",
    "next-redux-wrapper": "^7.0.5",
    "prop-types": "^15.7.2",
    "rdndmb-html5-to-touch": "^7.0.0-alpha.4",
    "react": "17.0.2",
    "react-aria-modal": "^4.0.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dnd-multi-backend": "^7.0.0-alpha.4",
    "react-dnd-touch-backend": "^14.1.1",
    "react-dom": "17.0.2",
    "react-image-crop": "^9.0.5",
    "react-query": "^3.34.5",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "slate": "^0.72.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.72.1",
    "socket.io-client": "^4.4.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "uuid-with-v6": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.16.5",
    "@types/enzyme": "^3.10.10",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.9",
    "@types/is-url": "^1.2.30",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-aria-modal": "^4.0.6",
    "@types/react-dnd-multi-backend": "^6.0.1",
    "@types/react-dom": "^17.0.11",
    "@types/react-image-crop": "^8.1.3",
    "@types/react-redux": "^7.1.20",
    "@types/slate": "^0.47.9",
    "@types/slate-react": "^0.50.1",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.3",
    "babel-jest": "^27.4.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "react-dnd-test-backend": "^14.0.1",
    "react-dnd-test-utils": "^14.0.2",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
